{
    "name": "tarfin-labs/easy-pdf",
    "description": "Makes pdf processing easy.",
    "keywords": [
        "tarfin-labs",
        "easy-pdf",
        "tcpdf",
        "laravel",
        "pdf",
        "qrcode",
        "html-to-pdf"
    ],
    "homepage": "https://github.com/tarfin-labs/easy-pdf",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Faruk Can",
            "email": "faruk.can@tarfin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/support": "6.11.0|^7.0|^8.0|^9.0|^10.0",
        "setasign/fpdi-tcpdf": "^2.2",
        "tecnickcom/tcpdf": "^6.3",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "TarfinLabs\\EasyPdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TarfinLabs\\EasyPdf\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TarfinLabs\\EasyPdf\\EasyPdfServiceProvider"
            ],
            "aliases": {
                "EasyPdf": "TarfinLabs\\EasyPdf\\EasyPdfFacade"
            }
        }
    }
}
